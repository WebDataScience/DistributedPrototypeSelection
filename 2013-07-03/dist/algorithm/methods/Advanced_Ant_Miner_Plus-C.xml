<algorithm_specification>
  <name>Advanced_Ant_Miner_Plus</name>
  <nParameters>8</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Number_of_Ants</name>
    <type>integer</type>
    <domain>
      <lowerB>50</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>1000</default>
  </parameter>
  <parameter>
    <name>Max_number_of_uncovered_samples</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>Min_number_of_samples_by_rule</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>500</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>Max_Iterations_without_converge</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>Max_Pheromona</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.999</default>
  </parameter>
  <parameter>
    <name>Min_Pheromona</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>Alpha</name>
    <type>real</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>3</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>Beta</name>
    <type>real</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>3</upperB>
    </domain>
    <default>2</default>
  </parameter>
</algorithm_specification>
