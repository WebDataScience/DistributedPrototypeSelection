<algorithm_specification>
  <name>Ant_Miner</name>
  <nParameters>4</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Number_of_Ants</name>
    <type>integer</type>
    <domain>
      <lowerB>50</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>3000</default>
  </parameter>
  <parameter>
    <name>Max_number_of_uncovered_samples</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>Min_number_of_samples_by_rule</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>500</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>Max_Iterations_without_converge</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>10</default>
  </parameter>
</algorithm_specification>
