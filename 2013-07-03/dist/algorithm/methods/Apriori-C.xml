<algorithm_specification>
  <name>Apriori-C</name>
  <nParameters>4</nParameters>
  <seed>0</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>MinSupport</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.03</default>
  </parameter>
  <parameter>
    <name>MinConfidence</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.9</default>
  </parameter>
  <parameter>
    <name>Number_of_Rules</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>50</upperB>
    </domain>
    <default>5</default>
  </parameter>
  <parameter>
    <name>Postpruning_type</name>
    <type>list</type>
    <domain>
      <item>SELECT_N_RULES</item>
      <item>SELECT_N_RULES_PER_CLASS</item>
    </domain>
    <default>SELECT_N_RULES_PER_CLASS</default>
  </parameter>
</algorithm_specification>
