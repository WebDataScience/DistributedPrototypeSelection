<algorithm_specification>
  <name>Bojarczuk_GP</name>
  <nParameters>5</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>population-size</name>
    <type>integer</type>
    <domain>
      <lowerB>50</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>200</default>
  </parameter>
  <parameter>
    <name>max-generations</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>200</default>
  </parameter>
  <parameter>
    <name>max-deriv-size</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>30</upperB>
    </domain>
    <default>20</default>
  </parameter> 
  <parameter>
    <name>rec-prob</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.8</default>
  </parameter> 
  <parameter>
    <name>copy-prob</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.01</default>
  </parameter>  
</algorithm_specification>
