<algorithm_specification>
  <name>Classification with Fuzzy Association Rules (CFAR)</name>
  <nParameters>4</nParameters>
  <seed>1</seed>
  <nOutput>2</nOutput>
  <parameter>
    <name>Minimum Support</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>Minimum Confidence</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.85</default>
  </parameter>
  <parameter>
    <name>Cut Threshold (MS)</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.15</default>
  </parameter>
  <parameter>
    <name>Number of Labels</name>
    <type>integer</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>5</default>
  </parameter>
</algorithm_specification>
