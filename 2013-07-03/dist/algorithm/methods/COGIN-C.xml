<algorithm_specification>
  <name>COGIN</name>
  <nParameters>4</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>missclassificationErrorLevel</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>2</default>
  </parameter>
  <parameter>
    <name>generationLimit</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>5000</upperB>
    </domain>
    <default>1000</default>
  </parameter>
  <parameter>
    <name>CrossoverRate</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.9</default>
  </parameter>
  <parameter>
    <name>useNegationBit</name>
    <type>list</type>
    <domain>
      <item>Yes</item>
      <item>No</item>
    </domain>
    <default>Yes</default>
  </parameter>
</algorithm_specification>
