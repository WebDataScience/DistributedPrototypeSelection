<algorithm_specification>
  <name>CORE</name>
  <nParameters>7</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>popSize</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>CopopulationSize</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>50</default>
  </parameter>
  <parameter>
    <name>generationLimit</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>500</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>numberOfCopopulations</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>500</upperB>
    </domain>
    <default>15</default>
  </parameter>
  <parameter>
    <name>CrossoverRate</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>1.0</default>
  </parameter>
  <parameter>
    <name>ProbMutation</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>RegenerationProbability</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
</algorithm_specification>
