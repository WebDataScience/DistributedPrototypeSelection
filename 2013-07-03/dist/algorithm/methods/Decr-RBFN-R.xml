<algorithm_specification>
  <name>Decremental Radial Basis Function Neural Network for Regression Problems</name>
  <nParameters>3</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>percent</name>
    <type>real</type>
    <domain>
      <lowerB>0.01</lowerB>
      <upperB>0.99</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>nNeuronsIni</name>
    <type>integer</type>
    <domain>
      <lowerB>5</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>20</default>
  </parameter>
  <parameter>
    <name>alfa</name>
    <type>real</type>
    <domain>
      <lowerB>0.1</lowerB>
      <upperB>0.9</upperB>
    </domain>
    <default>0.3</default>
  </parameter>
</algorithm_specification>
