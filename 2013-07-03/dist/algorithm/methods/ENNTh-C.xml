<algorithm_specification>
  <name>Edited Nearest Neighbor with Estimation of Probabilities Threshold</name>
  <nParameters>3</nParameters>
  <seed>0</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>Number of Neighbors</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>Noise Threshold</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.7</default>
  </parameter>
  <parameter>
    <name>Distance Function</name>
    <type>list</type>
    <domain>
      <item>Euclidean</item>
      <item>HVDM</item>
    </domain>
    <default>Euclidean</default>   
  </parameter>
</algorithm_specification>
