<algorithm_specification>
  <name>Ensemble Neural Network for Regression Problems</name>
  <nParameters>19</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Hidden_layers</name>
    <type>integer</type>
    <default>2</default>
  </parameter>
  <parameter>
    <name>Hidden_nodes</name>
    <type>integer</type>
    <default>15</default>
  </parameter>
  <parameter>
    <name>Transfer</name>
    <type>list</type>
    <domain>
      <item>Log</item>
      <item>Htan</item>
      <item>Lin</item>
    </domain>
    <default>Htan</default>
  </parameter>
  <parameter>
    <name>Eta</name>
    <type>real</type>
    <default>0.15</default>
  </parameter>
  <parameter>
    <name>Alpha</name>
    <type>real</type>
    <default>0.10</default>
  </parameter>
  <parameter>
    <name>Lambda</name>
    <type>real</type>
    <default>0.0</default>
  </parameter>
  <parameter>
    <name>Test_data</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>true</default>
    <disabled/>
  </parameter>
  <parameter>
    <name>Validation_data</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>true</default>
	<disabled/>    
  </parameter>
  <parameter>
    <name>Cross_validation</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>false</default>
    <disabled/>
  </parameter>
  <parameter>
    <name>Cycles</name>
    <type>integer</type>
    <default>10000</default>
  </parameter>
  <parameter>
    <name>Improve</name>
    <type>real</type>
    <default>0.01</default>
  </parameter>
  <parameter>
    <name>Problem</name>
    <type>list</type>
    <domain>
      <item>Classification</item>
      <item>Regression</item>
    </domain>
    <default>Regression</default>
	<disabled/>    
  </parameter>
  <parameter>
    <name>Tipify_inputs</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>true</default>
  </parameter>
  <parameter>
    <name>Verbose</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>false</default>
  </parameter>
  <parameter>
    <name>SaveAll</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>false</default>
  </parameter>
  <parameter>
    <name>Sampling</name>
    <type>list</type>
    <domain>
      <item>Ada</item>
      <item>Arcing</item>
      <item>Bagging</item>
      <item>None</item>
    </domain>
    <default>None</default>
  </parameter>
  <parameter>
    <name>Ensemble_method</name>
    <type>list</type>
    <domain>
      <item>BEM</item>
      <item>GEM</item>
    </domain>
    <default>BEM</default>
  </parameter>
  <parameter>
    <name>Combination</name>
    <type>list</type>
    <domain>
      <item>WeightedSum</item>
      <item>MajorityVoting</item>
    </domain>
    <default>WeightedSum</default>
  </parameter>
  <parameter>
    <name>Networks</name>
    <type>integer</type>
    <default>10</default>
  </parameter>
</algorithm_specification>
