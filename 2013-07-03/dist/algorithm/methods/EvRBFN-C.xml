<algorithm_specification>
  <name>Evolutionary Radial Basis Function Neural Network for Classification</name>
  <nParameters>8</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>neuronsRate</name>
    <type>real</type>
    <domain>
      <lowerB>0.01</lowerB>
      <upperB>0.99</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>validationRate</name>
    <type>real</type>
    <domain>
      <lowerB>0.01</lowerB>
      <upperB>0.99</upperB>
    </domain>
    <default>0.15</default>
  </parameter>
  <parameter>
    <name>popSize</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>2000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>tournamentSize</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>2</default>
  </parameter>
  <parameter>
    <name>replacementRate</name>
    <type>real</type>
    <domain>
      <lowerB>0.01</lowerB>
      <upperB>0.99</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>maxGenerations</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>xOverRate</name>
    <type>real</type>
    <domain>
      <lowerB>0.01</lowerB>
      <upperB>0.99</upperB>
    </domain>
    <default>0.9</default>
  </parameter>
  <parameter>
    <name>mutatorRate</name>
    <type>real</type>
    <domain>
      <lowerB>0.01</lowerB>
      <upperB>0.99</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
</algorithm_specification>
