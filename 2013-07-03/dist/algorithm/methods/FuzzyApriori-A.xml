<algorithm_specification>
  <name>FuzzyApriori</name>
  <nParameters>4</nParameters>
  <seed>0</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Number of Fuzzy Regions for Numeric Attributes</name>
    <type>integer</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>Use Max Operator for 1-Frequent Itemsets</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>false</default>
  </parameter>
  <parameter>
    <name>Minimum Support</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>Minimum Confidence</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.8</default>
  </parameter>
</algorithm_specification>
