<algorithm_specification>
  <name>G3PMI</name>
  <nParameters>5</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
   <parameter>
    <name>population-size</name>
    <type>integer</type>
    <domain>
      <lowerB>50</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>1000</default>
  </parameter>
  <parameter>
    <name>max-generations</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>max-deriv-size</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>50</default>
  </parameter> 
  <parameter>
    <name>rec-prob</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.9</default>
  </parameter> 
  <parameter>
    <name>mut-prob</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.3</default>
  </parameter> 
</algorithm_specification>
