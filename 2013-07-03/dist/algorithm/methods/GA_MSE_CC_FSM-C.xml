<algorithm_specification>
  <name>Genetic Algorithm for Editing k-NN with MSE estimation, clustered crossover and fast smart mutation</name>
  <nParameters>4</nParameters>
  <seed>1</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>Number of Generations</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>Number of Neighbours</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>Distance Function</name>
    <type>list</type>
    <domain>
      <item>Euclidean</item>
      <item>HVDM</item>
    </domain>
    <default>Euclidean</default>   
  </parameter>
</algorithm_specification>
