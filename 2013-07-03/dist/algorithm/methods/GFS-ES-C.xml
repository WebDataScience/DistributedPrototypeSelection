<algorithm_specification>
  <name>Fuzzy Rule Learning Algorithm Proposed by Shi, Eberhart and Chen</name>
  <nParameters>6</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Number of Labels</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>5</default>
  </parameter>
  <parameter>  
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>Number of Generations</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>50</default>
  </parameter>
  <parameter>
    <name>Maximum number of rules</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>30</default>
  </parameter>  
  <parameter>
    <name>Crossover Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.75</default>
  </parameter>
  <parameter>
    <name>Mutation Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.01</default>
  </parameter>
</algorithm_specification>
