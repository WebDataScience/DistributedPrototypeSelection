<algorithm_specification>
  <name>Fuzzy Rule Learning with Single Winner Inference, Logitboost Algorithm</name>
  <nParameters>5</nParameters>
  <seed>1</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>subAlgorithm</name>
    <type>list</type>
    <domain>
      <item>ClasifFuzzyMaxLogitBoost</item>
    </domain>
    <default>ClasifFuzzyMaxLogitBoost</default>
    <disabled/>
  </parameter>
  <parameter>
    <name>dataformat</name>
    <type>list</type>
    <domain>
      <item>keel</item>
    </domain>
    <default>keel</default>
    <disabled/>
  </parameter>
  <parameter>
    <name>numlabels</name>
    <type>integer</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>7</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>numrules</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>8</default>
  </parameter>
  <parameter>
    <name>outlabel</name>
    <type>list</type>
    <domain>
      <item>FMLB</item>
    </domain>
    <default>FMLB</default>
    <disabled/>
  </parameter>
</algorithm_specification>
