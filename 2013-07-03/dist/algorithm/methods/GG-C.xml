<algorithm_specification>
  <name>Prototipe Selection based on Gabriel Graphs</name>
  <nParameters>3</nParameters>
  <seed>0</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>Order of the Graph</name>
    <type>list</type>
    <domain>
      <item>1st_order</item>
      <item>2nd_order</item>
    </domain>
    <default>1st_order</default>   
  </parameter>
  <parameter>
    <name>Type of Selection</name>
    <type>list</type>
    <domain>
      <item>Edition</item>
      <item>Condensation</item>
    </domain>
    <default>Edition</default>   
  </parameter>
  <parameter>
    <name>Distance Function</name>
    <type>list</type>
    <domain>
      <item>Euclidean</item>
      <item>HVDM</item>
    </domain>
    <default>Euclidean</default>   
  </parameter>
</algorithm_specification>
