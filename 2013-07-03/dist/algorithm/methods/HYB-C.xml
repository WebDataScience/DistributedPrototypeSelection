<algorithm_specification>
  <name>Hybrid</name>
  <nParameters>25</nParameters>
  <seed>1</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>iterations_of_search</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>200</default>
  </parameter>
  <parameter>
    <name>iterations_of_optimal_search</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>1000</default>
  </parameter>
  <parameter>
    <name>percentage_prototypes_generated</name>
    <type>real</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>alpha_0</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>init_epsilon</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.0</default>
  </parameter>
  <parameter>
    <name>end_epsilon</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>1.0</default>
  </parameter>  
  <parameter>
    <name>delta_epsilon</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>init_window_width</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.0</default>
  </parameter>
  <parameter>
    <name>end_window_width</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>1.0</default>
  </parameter>  
  <parameter>
    <name>delta_window_width</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>    
  <parameter>
    <name>percent_of_set_in_training_partition</name>
    <type>real</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>80</default>
  </parameter>     
	<parameter>
    <name>Type_of_initial_selection</name>
    <type>list</type>
    <domain>
      <item>PNN</item>
      <item>SVM</item>
      <item>CNN</item>
      <item>VQ</item>      
    </domain>
    <default>SVM</default>   
  </parameter>
	<parameter>
		<name>CNN_parameter_K</name>
		<type>integer</type>
		<domain>
      <lowerB>1</lowerB>
      <upperB>5</upperB>
	  </domain>
    <default>1</default>
	</parameter>
  <parameter>
	  <name>VQ_parameter_Iterations</name>
 		<type>integer</type>
	  <domain>
      <lowerB>10</lowerB>
      <upperB>10000</upperB>
	  </domain>
    <default>100</default>
  </parameter>
  
  <parameter>
    <name>VQ_parameter_Percentage_of_reduction</name>
		<type>real</type>    
		<domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
	  </domain>
    <default>20</default>
  </parameter>
  <parameter>
  <name>VQ_parameter_K_in_KNN</name>
  	<type>integer</type>    
		<domain>
      <lowerB>1</lowerB>
      <upperB>5</upperB>
	  </domain>
    <default>2</default>
  </parameter>
  <parameter>
    <name>VQ_parameter_Alpha_0</name>
    <type>real</type>    
		<domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
	  </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
  	<name>SVM_parameter_Kernel_type</name>
    <type>list</type>
    <domain>
      <item>LINEAR</item>
      <item>POLY</item>
      <item>RBF</item>
      <item>SIGMOID</item>      
    </domain>
    <default>POLY</default>
  </parameter>
  <parameter>
    <name>SVM_parameter_C</name>
    <type>real</type>    
		<domain>
      <lowerB>0</lowerB>
      <upperB>1000</upperB>
	  </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>SVM_parameter_Epsilon</name>
    <type>real</type>    
		<domain>
      <lowerB>0</lowerB>
      <upperB>1000</upperB>
	  </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>SVM_parameter_Degree</name>
    <type>integer</type>    
		<domain>
      <lowerB>0</lowerB>
      <upperB>1000</upperB>
	  </domain>
    <default>3</default>
  </parameter>
  <parameter>
  	<name>SVM_parameter_Gamma</name>
  	<type>real</type>    
		<domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
	  </domain>
    <default>0.25</default>
  </parameter>
  <parameter>
  	<name>SVM_parameter_Nu</name>
  	<type>real</type>    
		<domain>
      <lowerB>0</lowerB>
      <upperB>1000</upperB>
	  </domain>
    <default>0.5</default>
  </parameter>
  <parameter>
    <name>SVM_parameter_P</name>
    <type>real</type>    
		<domain>
      <lowerB>0</lowerB>
      <upperB>100</upperB>
	  </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>SVM_parameter_Shrinking</name>
    <type>integer</type>    
		<domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
	  </domain>
    <default>1</default>
  </parameter>       
</algorithm_specification>
