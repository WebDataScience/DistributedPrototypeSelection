<algorithm_specification>
  <name>ICFPSO</name>
  <nParameters>9</nParameters>
  <seed>1</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>Number of Neighbours</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>Distance Function</name>
    <type>list</type>
    <domain>
      <item>Euclidean</item>
      <item>HVDM</item>
    </domain>
    <default>Euclidean</default>   
  </parameter>


   <parameter>
    <name>SwarmSize</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>50</default>
  </parameter>
  
  
   <parameter>
    <name>MaxIter</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>500</default>
  </parameter>
  
   <parameter>
    <name>C1</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>5</upperB>
    </domain>
    <default>1</default>
  </parameter>
  
  <parameter>
    <name>C2</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>5</upperB>
    </domain>
    <default>3</default>
  </parameter>
  
  
  <parameter>
    <name>VMax</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.25</default>
  </parameter>
  
  <parameter>
    <name>Wstart</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>5</upperB>
    </domain>
    <default>1.5</default>
  </parameter>
  
  <parameter>
    <name>Wend</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>5</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
  
</algorithm_specification>
