<algorithm_specification>
  <name>Intelligent Genetic Algorithm for Edition</name>
  <nParameters>7</nParameters>
  <seed>1</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>1 to 0 Mutation Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.01</default>
  </parameter>
  <parameter>
    <name>0 to 1 Mutation Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.001</default>
  </parameter>
  <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>Number of Evaluations</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000000</upperB>
    </domain>
    <default>10000</default>
  </parameter>
  <parameter>
    <name>Alpha Weight for Fitness</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
  <parameter>
    <name>Number of Neighbours</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>Distance Function</name>
    <type>list</type>
    <domain>
      <item>Euclidean</item>
      <item>HVDM</item>
    </domain>
    <default>Euclidean</default>   
  </parameter>
</algorithm_specification>
