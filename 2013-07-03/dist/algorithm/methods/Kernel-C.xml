<algorithm_specification>
  <name>Kernel Classifier</name>
  <nParameters>4</nParameters>
  <seed>1</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>subAlgorithm</name>
    <type>list</type>
    <domain>
      <item>ClasifKernel</item>
    </domain>
    <default>ClasifKernel</default>
    <disabled/>
  </parameter>
  <parameter>
    <name>dataformat</name>
    <type>list</type>
    <domain>
      <item>keel</item>
    </domain>
    <default>keel</default>
    <disabled/>
  </parameter>
  <parameter>
    <name>outlabel</name>
    <type>list</type>
