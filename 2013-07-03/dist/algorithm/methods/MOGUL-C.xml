<algorithm_specification>
  <name>Fuzzy Rule Learning, MOGUL Algorithm</name>
  <nParameters>27</nParameters>
  <seed>1</seed>
  <nOutput>4</nOutput>
  <parameter>
    <name>Number of Labels</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>5</default>
  </parameter>
  <parameter>
    <name>Omega parameter for the maching degree of the positive instances</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.05</default>
  </parameter>
  <parameter>
    <name>K parameter for the percentage of allowed negative instances</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>Epsilon parameter for the minimun maching degree required to the KB</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>3</upperB>
    </domain>
    <default>1.5</default>
  </parameter>
  <parameter>
    <name>Repeated rules</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>Type of Rules</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>3</upperB>
    </domain>
    <default>2</default>
  </parameter>
  <parameter>
    <name>Way of obtaining the matching degree of an example with the antedecent of a rule</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>3</upperB>
    </domain>
    <default>0</default>
  </parameter>
  <parameter>
    <name>Type of FRM</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>8</upperB>
    </domain>
    <default>0</default>
  </parameter>
  <parameter>
    <name>Alfa parameter for the FRM</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.0</default>
  </parameter>
  <parameter>
    <name>p parameter for the FRM</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.0</default>
  </parameter>
  <parameter>
    <name>a parameter for the FRM</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.0</default>
  </parameter>
  <parameter>
    <name>b parameter for the FRM</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.0</default>
  </parameter>
  <parameter>
    <name>Type of Linguistic Hedges</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>2</upperB>
    </domain>    
    <default>0</default>
  </parameter>
  <parameter>  
    <name>Number of Iterations SELECTION</name>
    <type>integer</type>
    <domain>
      <lowerB>20</lowerB>
      <upperB>2000</upperB>
    </domain>    
    <default>500</default>
  </parameter>
  <parameter>
    <name>Population Size SELECTION</name>
    <type>integer</type>
      <lowerB>10</lowerB>
      <upperB>200</upperB>
    <default>61</default>
  </parameter>
  <parameter>
    <name>Tau parameter for the minimun maching degree required to the KB SELECTION</name>
    <type>real</type>
      <lowerB>0.0</lowerB>
      <upperB>100.0</upperB>
    <default>1.5</default>
  </parameter>
  <parameter>
    <name>Rate of Rules that don't are eliminated SELECTION</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.0</default>
  </parameter>
  <parameter>
    <name>Rate of rules to estimate the niche radio SELECTION</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.025</default>
  </parameter>
  <parameter>
    <name>Beta parameter for the Power Law SELECTION</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
  <parameter>
    <name>Cross Probability SELECTION</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.6</default>
  </parameter>
  <parameter>
    <name>Mutation Probability SELECTION</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>Number of Iterations TUNING</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>50000</upperB>
    </domain>
    <default>1000</default>
  </parameter>
  <parameter>
    <name>Population Size TUNING</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>61</default>
  </parameter>
  <parameter>
    <name>Parameter a TUNING</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.35</default>
  </parameter>
  <parameter>
    <name>Parameter b TUNING</name>
    <type>real</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>10</upperB>
    </domain>
    <default>5</default>
  </parameter>
  <parameter>
    <name>Cross Probability TUNING</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.6</default>
  </parameter>
  <parameter>
    <name>Mutation Probability TUNING</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
</algorithm_specification>
