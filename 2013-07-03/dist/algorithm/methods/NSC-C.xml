<algorithm_specification>
  <name>Nearest Subclass Classifier</name>
  <nParameters>5</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>K Value</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>Q Value</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>NoChange</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>Epoch</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>100</default>
  </parameter>
    <parameter>
    <name>Variance</name>
    <type>real</type>
    <domain>
      <lowerB>0.0001</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>0.01</default>
  </parameter>
</algorithm_specification>
