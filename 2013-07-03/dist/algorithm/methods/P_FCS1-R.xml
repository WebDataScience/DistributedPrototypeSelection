<algorithm_specification>
  <name>A Pittsburgh-style fuzzy classifier system (P-FCS1)</name>
  <nParameters>5</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>Initial number of rules per individual</name>
    <type>integer</type>
    <domain>
      <lowerB>5</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>40</default>
  </parameter>  
  <parameter>
    <name>Number of Generations</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>100000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>Crossover Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.8</default>
  </parameter>
  <parameter>
    <name>Mutation Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
</algorithm_specification>
