<algorithm_specification>
  <name>SADE</name>
  <nParameters>6</nParameters>
  <seed>1</seed>
  <nOutput>0</nOutput>

  <parameter>
    <name>Number of Neighbors</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>1</default>
  </parameter>

   <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>50</default>
  </parameter>
  
   <parameter>
    <name>Particle Size</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>2</default>
  </parameter>
  
   <parameter>
    <name>MaxIter</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>500</default>
  </parameter>
  
   <parameter>
    <name>Learning Period</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>100</default>
  </parameter>

   <parameter>
    <name>number of strategies</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>7</upperB>
    </domain>
    <default>4</default>
  </parameter>   
  
</algorithm_specification>
