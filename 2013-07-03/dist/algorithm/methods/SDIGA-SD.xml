    <domain>
      <item>can</item>
      <item>dnf</item>
    </domain>
    <default>can</default>
  </parameter>
  <parameter>
    <name>nLabels</name>
    <type>integer</type>
	<domain>
	  <lowerB>1</lowerB>
	  <upperB>9</upperB>
	</domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>nEval</name>
    <type>integer</type>
    <default>10000</default>
  </parameter>
  <parameter>
    <name>popLength</name>
    <type>integer</type>
    <default>100</default>
  </parameter>
  <parameter>
    <name>crossProb</name>
    <type>real</type>
	<domain>
	  <lowerB>0</lowerB>
	  <upperB>1</upperB>
	</domain>
    <default>0.6</default>
  </parameter>
  <parameter>
    <name>mutProb</name>
    <type>real</type>
	<domain>
	  <lowerB>0</lowerB>
	  <upperB>1</upperB>
