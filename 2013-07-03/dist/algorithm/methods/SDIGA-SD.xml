  </parameter>
  <parameter>
    <name>lSearch</name>
    <type>list</type>
    <domain>
      <item>yes</item>
      <item>no</item>
    </domain>
    <default>yes</default>
  </parameter>
  <parameter>
    <name>Obj1</name>
    <type>list</type>
    <domain>
      <item>comp</item>
      <item>csup</item>
      <item>fsup</item>
    </domain>
    <default>comp</default>
  </parameter>
  <parameter>
    <name>Obj2</name>
    <type>list</type>
    <domain>
      <item>ccnf</item>
      <item>fcnf</item>
    </domain>
    <default>fcnf</default>
</parameter>
  <parameter>
    <name>Obj3</name>
    <type>list</type>
    <domain>
      <item>null</item>
      <item>unus</item>
      <item>cove</item>
      <item>accu</item>
      <item>sign</item>
    </domain>
    <default>unus</default>
  </parameter>
  <parameter>
    <name>w1</name>
    <type>real</type>
	<domain>
	  <lowerB>0</lowerB>
	  <upperB>1</upperB>
	</domain>
    <default>0.4</default>
  </parameter>
  <parameter>
    <name>w2</name>
    <type>real</type>
	<domain>
	  <lowerB>0</lowerB>
	  <upperB>1</upperB>
	</domain>
    <default>0.3</default>
  </parameter>
  <parameter>
    <name>w3</name>
    <type>real</type>
	<domain>
	  <lowerB>0</lowerB>
	  <upperB>1</upperB>
	</domain>
    <default>0.3</default>
  </parameter>
</algorithm_specification>
