<algorithm_specification>
  <name>Genetic reinforcement learning through Symbiotic Evolution for Fuzzy Controller (SEFC) desing</name>
  <nParameters>7</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>Number of Generations</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>100000</upperB>
    </domain>
    <default>50</default>
  </parameter>
  <parameter>
    <name>Number of generations to migration</name>
    <type>integer</type>
    <domain>
      <lowerB>5</lowerB>
      <upperB>50000</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>Number of fuzzy rules that constitute a fuzzy system</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>50</default>
  </parameter>
  <parameter>
    <name>Number of fuzzy systems to be formed and evaluated per generation</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>Parameter K</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>30</default>
  </parameter>
  <parameter>
    <name>Mutation Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.01</default>
  </parameter>
</algorithm_specification>
