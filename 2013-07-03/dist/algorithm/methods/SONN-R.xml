<algorithm_specification>
  <name>SONN Neural Networks</name>
  <nParameters>14</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Cross_validation</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>false</default>
  </parameter>
  <parameter>
    <name>Problem</name>
    <type>list</type>
    <domain>
      <item>Regression</item>
    </domain>
    <default>Regression</default>
    <disabled/>
  </parameter>
  <parameter>
    <name>Test_data</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>true</default>
  </parameter>
  <parameter>
    <name>Tipify_inputs</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>true</default>
  </parameter>
  <parameter>
    <name>Tend</name>
    <type>real</type>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>Omega</name>
    <type>integer</type>
    <default>5</default>
  </parameter>
  <parameter>
    <name>MaxNodes</name>
    <type>integer</type>
    <default>1000</default>
  </parameter>
  <parameter>
    <name>To</name>
    <type>real</type>
    <default>1.0</default>
  </parameter>
  <parameter>
    <name>alpha</name>
    <type>real</type>
    <default>0.99</default>
  </parameter>
  <parameter>
    <name>a_Range</name>
    <type>real</type>
    <default>5.0</default>
  </parameter>
  <parameter>
    <name>LM_convergence</name>
    <type>real</type>
    <default>0.0001</default>
  </parameter>
  <parameter>
    <name>Error</name>
    <type>list</type>
    <domain>
      <item>mse</item>
      <item>missclass</item>
    </domain>
    <default>missclass</default>
  </parameter>
  <parameter>
    <name>w_mse</name>
    <type>real</type>
    <default>1.0</default>
  </parameter>
  <parameter>
    <name>w_k</name>
    <type>real</type>
    <default>0.0</default>
  </parameter>
</algorithm_specification>
