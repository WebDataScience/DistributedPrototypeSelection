<algorithm_specification>
  <name>Suport Vectors Based Prototype Selection</name>
  <nParameters>10</nParameters>
  <seed>0</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>KERNELtype</name>
    <type>list</type>
    <domain>
      <item>LINEAR</item>
      <item>POLY</item>
      <item>RBF</item>
      <item>SIGMOID</item>
    </domain>
    <default>RBF</default>
  </parameter>
  <parameter>
    <name>C</name>
    <type>real</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100000</upperB>
    </domain>
    <default>1000.0</default>
  </parameter>
   <parameter>
    <name>eps</name>
    <type>real</type>
    <domain>
      <lowerB>0.000001</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.001</default>
  </parameter>
  <parameter>
    <name>degree</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>gamma</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>1.0</default>
  </parameter>
  <parameter>
    <name>nu</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
  <parameter>
    <name>p</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>1.0</default>
  </parameter>
  <parameter>
    <name>shrinking</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0</default>
  </parameter>
  <parameter>
    <name>Number of Neighbors</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>Distance Function</name>
    <type>list</type>
    <domain>
      <item>Euclidean</item>
      <item>HVDM</item>
    </domain>
    <default>Euclidean</default>   
  </parameter>
</algorithm_specification>
