<algorithm_specification>
  <name>Tan_GP</name>
  <nParameters>10</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>population-size</name>
    <type>integer</type>
    <domain>
      <lowerB>50</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>150</default>
  </parameter>
  <parameter>
    <name>max-generations</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>max-deriv-size</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>30</upperB>
    </domain>
    <default>20</default>
  </parameter> 
  <parameter>
    <name>rec-prob</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.8</default>
  </parameter> 
  <parameter>
    <name>mut-prob</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>   
  <parameter>
    <name>copy-prob</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.01</default>
  </parameter>  
  <parameter>
    <name>w1</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.7</default>
  </parameter>   
  <parameter>
    <name>w2</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.8</default>
  </parameter>  
  <parameter>
    <name>elitist-prob</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.06</default>
  </parameter> 
  <parameter>
    <name>support</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.03</default>
  </parameter>       
</algorithm_specification>
