<algorithm_specification>
  <name>Tree Analysis with Randomly Generated and Evolved Trees (TARGET)</name>
  <nParameters>6</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Probability of splitting a node</name>
    <type>real</type>
	<domain>     
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
  <parameter>
    <name>Number of Total Generations for the GA</name>
    <type>integer</type>
	<domain>    
      <lowerB>10</lowerB>
      <upperB>1000</upperB>
	</domain>      
    <default>100</default>
  </parameter>
  <parameter>
    <name>Number of Trees generated by crossover</name>
    <type>integer</type>
	<domain>    
      <lowerB>5</lowerB>
      <upperB>50</upperB>
	</domain>          
    <default>30</default>    
  </parameter>
  <parameter>
    <name>Number of Trees generated by mutation</name>
    <type>integer</type>
	<domain>    
      <lowerB>1</lowerB>
      <upperB>30</upperB>    
	</domain>      
    <default>10</default>
  </parameter>
  <parameter>
    <name>Number of Trees generated by clonation</name>
    <type>integer</type>
	<domain>    
      <lowerB>1</lowerB>
      <upperB>20</upperB>    
	</domain>      
    <default>5</default>
  </parameter>
  <parameter>
    <name>Number of Trees generated by immigration</name>
    <type>integer</type>
	<domain>    
      <lowerB>1</lowerB>
      <upperB>20</upperB>    
	</domain>      
    <default>5</default>
  </parameter>  
</algorithm_specification>
