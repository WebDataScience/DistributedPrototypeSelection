<algorithm_specification>

	<name>xcs</name>

	<nParameters>17</nParameters>
  
	<seed>1</seed>
	<nOutput>1</nOutput>
  

	<parameter>
		<name>numberOfExplores</name>
		<type>integer</type>
		<domain>
			<lowerB>1000</lowerB>
			<upperB>100000000</upperB>
		</domain>
		<default>100000</default>
	</parameter>

	<parameter>
		<name>popSize</name>
		<type>integer</type>
		<domain>
			<lowerB>50</lowerB>
			<upperB>50000</upperB>
		</domain>
		<default>6400</default>
	</parameter>

	<parameter>
		<name>delta</name>
		<type>real</type>
		<domain>
			<lowerB>0</lowerB>
			<upperB>1</upperB>
		</domain>
		<default>0.1</default>
	</parameter>

	<parameter>
		<name>nu</name>
		<type>real</type>
		<domain>
			<lowerB>1.0</lowerB>
			<upperB>20.0</upperB>
		</domain>
		<default>10.0</default>
	</parameter>

	<parameter>
		<name>acc_0</name>
		<type>real</type>
		<domain>
			<lowerB>0.1</lowerB>
			<upperB>1</upperB>
		</domain>
		<default>0.99</default>
	</parameter>

	<parameter>
		<name>pX</name>
		<type>real</type>
		<domain>
			<lowerB>0</lowerB>
			<upperB>1</upperB>
		</domain>
		<default>0.8</default>
	</parameter>

	<parameter>
		<name>pM</name>
		<type>real</type>
		<domain>
			<lowerB>0</lowerB>
			<upperB>1</upperB>
		</domain>
		<default>0.04</default>
	</parameter>

	<parameter>
		<name>theta_ga</name>
		<type>real</type>
		<domain>
			<lowerB>10</lowerB>
			<upperB>1000</upperB>
		</domain>
		<default>50.0</default>
	</parameter>
  
	<parameter>
		<name>theta_del</name>
		<type>real</type>
		<domain>
			<lowerB>10</lowerB>
			<upperB>10000</upperB>
		</domain>
		<default>50.0</default>
	</parameter>

	<parameter>
		<name>theta_sub</name>
		<type>real</type>
		<domain>
			<lowerB>10</lowerB>
			<upperB>10000</upperB>
		</domain>
		<default>500.0</default>
	</parameter>

	<parameter>
		<name>doGASubsumption</name>
		<type>list</type>
		<domain>
			<item>true</item>
			<item>false</item>
		</domain>
		<default>true</default>
	</parameter>

	<parameter>
		<name>typeOfSelection</name>
		<type>list</type>
		<domain>
			<item>RWS</item>
			<item>Tournament</item>
		</domain>
		<default>RWS</default>
	</parameter>

	<parameter>
		<name>tournamentSize</name>
		<type>real</type>
		<domain>
			<lowerB>0</lowerB>
			<upperB>1</upperB>
		</domain>
		<default>0.4</default>
	</parameter>

	<parameter>
		<name>typeOfMutation</name>
		<type>list</type>
		<domain>
			<item>niched</item>
			<item>free</item>
		</domain>
		<default>free</default>
	</parameter>

	<parameter>
		<name>typeOfCrossover</name>
		<type>list</type>
		<domain>
			<item>2PT</item>
			<item>uniform</item>
		</domain>
		<default>2PT</default>
	</parameter>

	<parameter>
		<name>r_0</name>
		<type>real</type>
		<domain>
			<lowerB>0</lowerB>
			<upperB>1</upperB>
		</domain>
		<default>0.6</default>
	</parameter>

	<parameter>
		<name>m_0</name>
		<type>real</type>
		<domain>
			<lowerB>0</lowerB>
			<upperB>1</upperB>
		</domain>
		<default>0.1</default>
	</parameter>

</algorithm_specification>
