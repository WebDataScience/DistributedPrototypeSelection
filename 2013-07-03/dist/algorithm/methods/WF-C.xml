<algorithm_specification>
  <name>Weighted Fuzzy Classifier</name>
  <nParameters>5</nParameters>
  <seed>0</seed>
  <nOutput>2</nOutput>
  <parameter>
    <name>Number of Fuzzy Labels</name>
    <type>integer</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>5</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>Cost for the majority classes</name>
    <type>list</type>
    <domain>
      <item>NONE</item>
      <item>HALF_MINORITY</item>
      <item>PROPORTIONAL</item>
    </domain>
    <default>PROPORTIONAL</default>
  </parameter>
  <parameter>
    <name>Apply learning of the rule weights</name>
    <type>list</type>
    <domain>
      <item>YES</item>
      <item>NO</item>
    </domain>
    <default>YES</default>
  </parameter>
  <parameter>
    <name>nu</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.02</default>
  </parameter>  
   <parameter>
    <name>epochs</name>
    <type>integer</type>
    <domain>
      <lowerB>5</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>10</default>
   </parameter>
</algorithm_specification>
