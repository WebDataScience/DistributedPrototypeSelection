<algorithm_specification>
  <name>Genetic-Based New Fuzzy Reasoning Model</name>
  <nParameters>4</nParameters>
  <seed>1</seed>
  <nOutput>3</nOutput>
  <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>5</lowerB>
      <upperB>500</upperB>
    </domain>
    <default>50</default>
  </parameter>
  <parameter>
    <name>Number of Generations</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>Crossover probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.9</default>
  </parameter>
  <parameter>
    <name>Mutation Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
</algorithm_specification>
