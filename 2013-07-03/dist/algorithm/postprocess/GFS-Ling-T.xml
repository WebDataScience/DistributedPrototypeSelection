<algorithm_specification>
  <name>Global Genetic Tuning of the Fuzzy Partition of Linguistic FRBSs</name>
  <nParameters>6</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Number of Iterations</name>
    <type>integer</type>
    <domain>
      <lowerB>10</lowerB>
      <upperB>50000</upperB>
    </domain>
    <default>1000</default>
  </parameter>
  <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>5</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>61</default>
  </parameter>
  <parameter>
    <name>Parameter a</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.35</default>
  </parameter>
  <parameter>
    <name>Parameter b</name>
    <type>real</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>10</upperB>
    </domain>
    <default>5</default>
  </parameter>
  <parameter>
    <name>Cross Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.6</default>
  </parameter>
  <parameter>
    <name>Mutation Probability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
</algorithm_specification>
