<algorithm_specification>
  <name>Class-Atribute Dependent Discretizer</name>
  <nParameters>2</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>confidenceThreshold</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.01</default>
  </parameter>
  <parameter>
    <name>numIntervals</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>0</default>
  </parameter>
</algorithm_specification>
