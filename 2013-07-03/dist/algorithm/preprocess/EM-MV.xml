<algorithm_specification>
  <name>EM interpolation for Imputation</name>
  <nParameters>8</nParameters>
  <seed>0</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>RegrParameter</name>
    <type>real</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>1.0</default>
  </parameter>
   <parameter>
    <name>MaxIter</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>30</default>
  </parameter>
  <parameter>
    <name>RegressionType</name>
    <type>list</type>
    <domain>
      <item>mridge</item>
      <item>iridge</item>
      <item>ttls</item>
    </domain>
    <default>mridge</default>
  </parameter>
   <parameter>
    <name>StagnationTolerance</name>
    <type>real</type>
    <domain>
      <lowerB>0.000001</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.0001</default>
  </parameter>
    <parameter>
    <name>NumberOfEigens</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>0</default>
  </parameter>
  <parameter>
    <name>MinimumFractionOfTotalVariation</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>0.0</default>
  </parameter>
  <parameter>
    <name>CovMatrixInflationFactor</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>1.0</default>
  </parameter>
    <parameter>
    <name>UseRegPar</name>
    <type>list</type>
    <domain>
      <item>Yes</item>
      <item>No</item>
    </domain>
    <default>No</default>
  </parameter>
</algorithm_specification>
