<algorithm_specification>
  <name>Generational GA with binary coding scheme for filter feature selection with the inconsistency rate</name>
  <nParameters>7</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>paramKNN</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>7</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>nEval</name>
    <type>integer</type>
    <domain>
      <lowerB>1000</lowerB>
      <upperB>100000</upperB>
    </domain>
    <default>5000</default>
  </parameter>
  <parameter>
    <name>popLength</name>
    <type>integer</type>
    <domain>
      <lowerB>20</lowerB>
      <upperB>500</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>alfa</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>crossProb</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.6</default>
  </parameter>
  <parameter>
    <name>mutProb</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.01</default>
  </parameter>
  <parameter>
    <name>k</name>
    <type>integer</type>
    <domain>
      <lowerB>3</lowerB>
      <upperB>30</upperB>
    </domain>
    <default>5</default>
  </parameter>
</algorithm_specification>
