<algorithm_specification>
  <name>Multivariate Discretizer</name>
  <nParameters>2</nParameters>
  <seed>0</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>numIntervals</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>alpha</name>
    <type>real</type>
    <domain>
      <lowerB>0.00001</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.05</default>
  </parameter>
</algorithm_specification>
