<algorithm_specification>
  <name>Population-Based Incremental Learning</name>
  <nParameters>10</nParameters>
  <seed>1</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>nGenerations</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100000</upperB>
    </domain>
    <default>10000</default>
  </parameter>
  <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>50</default>
  </parameter>
  <parameter>
    <name>LR</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>Mut_shift</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.05</default>
  </parameter>
  <parameter>
    <name>mutationProbability</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.02</default>
  </parameter>
  <parameter>
    <name>Negative_LR</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.075</default>
  </parameter>
  <parameter>
    <name>alphaEquilibrateFactor</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
  <parameter>
    <name>initialValueofPV</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.3</default>
  </parameter>
  <parameter>
    <name>Number of Neighbours</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>9</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>Distance Function</name>
    <type>list</type>
    <domain>
      <item>Euclidean</item>
      <item>HVDM</item>
    </domain>
    <default>Euclidean</default>   
  </parameter>
</algorithm_specification>
