<algorithm_specification>
  <name>Relief</name>
  <nParameters>3</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>paramKNN</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>7</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>relevanceThreshold</name>
    <type>real</type>
    <default>0.20</default>
  </parameter>
  <parameter>
    <name>numInstancesSampled</name>
    <type>integer</type>
    <default>100</default>
  </parameter>
</algorithm_specification>
