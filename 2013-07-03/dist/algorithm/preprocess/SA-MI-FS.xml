<algorithm_specification>
  <name>Simulated Annealing (Mutual Information)</name>
  <nParameters>4</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>tInit</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>100000000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>maxLoops</name>
    <type>integer</type>
    <default>100</default>
  </parameter>
  <parameter>
    <name>neighbors</name>
    <type>integer</type>
    <default>20</default>
  </parameter>
  <parameter>
    <name>beta</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
</algorithm_specification>
