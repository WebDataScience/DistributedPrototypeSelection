<algorithm_specification>
  <name>SMOTE RSB</name>
  <nParameters>9</nParameters>
  <seed>1</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>Number of Neighbors</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>900</upperB>
    </domain>
    <default>5</default>
  </parameter>
  <parameter>
    <name>Type of neighbors</name>
    <type>list</type>
    <domain>
      <item>Both</item>
      <item>Minority</item>
      <item>Majority</item>
    </domain>
    <default>Both</default>   
  </parameter>
    <parameter>
      <name>Balance</name>
      <type>list</type>
      <domain>
        <item>Yes</item>
        <item>No</item>
      </domain>
      <default>Yes</default>   
  </parameter>
  <parameter>
      <name>Smoting</name>
      <type>real</type>
      <domain>
        <lowerB>0</lowerB>
        <upperB>1</upperB>
      </domain>
      <default>1</default>
  </parameter>
      <parameter>
    <name>Number of Extension</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>6</upperB>
    </domain>
    <default>1</default>
  </parameter>
   <parameter>
      <name>Comptype</name>
      <type>integer</type>
      <domain>
        <lowerB>0</lowerB>
        <upperB>1</upperB>
      </domain>
      <default>1</default>
  </parameter>
    <parameter>
        <name>Cutoffini</name>
        <type>real</type>
        <domain>
          <lowerB>0</lowerB>
          <upperB>1</upperB>
        </domain>
        <default>0.6</default>
  </parameter>
  <parameter>
        <name>Cutoffinal</name>
        <type>real</type>
        <domain>
          <lowerB>0</lowerB>
          <upperB>1</upperB>
        </domain>
        <default>0.9</default>
  </parameter>
  <parameter>
          <name>MustContinue</name>
          <type>list</type>
          <domain>
            <lowerB>Yes</lowerB>
            <upperB>No</upperB>
          </domain>
          <default>Yes</default>
  </parameter>
</algorithm_specification>
