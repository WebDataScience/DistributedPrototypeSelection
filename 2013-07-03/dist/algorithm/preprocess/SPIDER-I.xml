<algorithm_specification>
  <name>SPIDER</name>
  <nParameters>3</nParameters>
  <seed>0</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>Number of Neighbors</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>20</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>Distance Function</name>
    <type>list</type>
    <domain>
      <item>Euclidean</item>
      <item>HVDM</item>
    </domain>
    <default>HVDM</default>   
  </parameter>
  <parameter>
    <name>Preprocessing Option</name>
    <type>list</type>
    <domain>
      <item>WEAK</item>
      <item>RELABEL</item>
	  <item>STRONG</item>
    </domain>
    <default>WEAK</default>   
  </parameter>
</algorithm_specification>

