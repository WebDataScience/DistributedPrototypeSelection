<algorithm_specification>
  <name>SPIDER2</name>
  <nParameters>4</nParameters>
  <seed>0</seed>
  <nOutput>0</nOutput>
  <parameter>
    <name>Number of Neighbors</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>20</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>Distance Function</name>
    <type>list</type>
    <domain>
      <item>Euclidean</item>
      <item>HVDM</item>
    </domain>
    <default>HVDM</default>   
  </parameter>
  <parameter>
    <name>relabel</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>true</default>   
  </parameter>
    <parameter>
    <name>ampl</name>
    <type>list</type>
    <domain>
      <item>no</item>
      <item>weak</item>
	  <item>strong</item>
    </domain>
    <default>strong</default>   
  </parameter>
</algorithm_specification>

