<algorithm_specification>
  <name>UCPD</name>
  <nParameters>8</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>maxIntervals</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>6</default>
  </parameter>
  <parameter>
    <name>minIntervals</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>mapType</name>
    <type>list</type>
    <domain>
      <item>knn</item>
      <item>projection</item>
    </domain>
    <default>knn</default>
  </parameter>
  <parameter>
    <name>Neighborhood</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>6</default>
  </parameter>
  <parameter>
    <name>minSupport</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>25</default>
  </parameter>
  <parameter>
    <name>mergedThreshold</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
  <parameter>
    <name>scalingFactor</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
  <parameter>
    <name>useDiscrete</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>true</default>
  </parameter>
</algorithm_specification>
